{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/norm/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/norm/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/norm",
  "projects": {
    "norm": {
      "type": "application",
      "root": "apps/norm",
      "entryFile": "main",
      "sourceRoot": "apps/norm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/norm/tsconfig.app.json"
      }
    },
    "times": {
      "type": "application",
      "root": "apps/times",
      "entryFile": "main",
      "sourceRoot": "apps/times/src",
      "compilerOptions": {
        "tsConfigPath": "apps/times/tsconfig.app.json"
      }
    },
    "command": {
      "type": "library",
      "root": "libs/command",
      "entryFile": "index",
      "sourceRoot": "libs/command/src",
      "compilerOptions": {
        "tsConfigPath": "libs/command/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "sdk": {
      "type": "library",
      "root": "libs/sdk",
      "entryFile": "index",
      "sourceRoot": "libs/sdk/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sdk/tsconfig.lib.json"
      }
    }
  }
}
